{
  "name": "k-web-theme",
  "version": "1.0.0",
  "description": "",
  "targets": {
    "default": {
      "includeNodeModules": true
    }
  },
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build": "npm run build-icons && npm run build-css && npm run build-js",
    "build-icons": "node build-icons.js",
    "build-css": "parcel build --no-source-maps --no-autoinstall --dist-dir ./public_content/assets/css --public-url /assets/css ./static_content/assets/sass/index.scss",
    "build-js": "parcel build --no-source-maps --no-autoinstall --dist-dir ./public_content/assets/js --public-url /assets/js ./static_content/assets/js/index.js",
    "build-style": "npm run build-icons && npm run build-css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runtimeverification/k-web-theme.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/runtimeverification/k-web-theme/issues"
  },
  "homepage": "https://github.com/runtimeverification/k-web-theme#readme",
  "dependencies": {
    "@shd101wyy/mume": "^0.7.2",
    "animate.css": "^4.1.1",
    "ansi_up": "^4.0.4",
    "bootstrap": "^4.5.3",
    "bootstrap-select": "^1.13.18",
    "cheerio": "^1.0.0-rc.3",
    "clipboard": "^2.0.6",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "get-port": "^5.1.1",
    "glob": "^7.1.6",
    "http-server": "^0.12.3",
    "imagesloaded": "^4.1.4",
    "isotope-layout": "^3.0.6",
    "jquery": "^3.5.1",
    "jquery-countdown": "^2.2.0",
    "jquery-visible": "^1.2.0",
    "jquery.animate-number": "0.0.14",
    "katex": "^0.13.13",
    "magnific-popup": "^1.1.0",
    "markdown-it": "^12.0.2",
    "markdown-it-anchor": "^6.0.1",
    "markdown-it-attrs": "^4.1.0",
    "owl.carousel": "^2.3.4",
    "popper.js": "^1.16.1",
    "prismjs": "^1.22.0",
    "sass": "^1.29.0",
    "sitemap-generator": "^8.5.1",
    "toastr": "^2.1.4",
    "twitter-fetcher": "^18.0.4",
    "webfonts-generator": "^0.4.0",
    "wow.js": "^1.2.2",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.7.0",
    "parcel": "^2.7.0"
  }
}
